{
  "commit_type": {
    "append_emoji_to_label": true,
    "append_emoji_to_commit": false
  },
  "commit_scope": {
    "enable": true,
    "initial_value": "main",
    "options": [
      {
        "value": "main",
        "label": "main"
      },
      {
        "value": "branch",
        "label": "branch"
      },
      {
        "value": "init",
        "label": "init"
      },
      {
        "value": "util",
        "label": "util"
      },
      {
        "value": "build",
        "label": "build"
      },
      {
        "value": "",
        "label": "none"
      }
    ]
  },
  "check_ticket": {
    "append_hashtag": true
  },
  "branch_pre_commands": [
    "git stash",
    "git checkout main",
    "git pull -r origin main",
    "npm install"
  ]
}
